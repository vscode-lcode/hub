    "@types/vscode": "^1.68.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "vsce": "^2.9.2"
  },
  "dependencies": {
    "eventsource": "^2.0.2",
    "node-fetch-commonjs": "^3.1.1"
  }
