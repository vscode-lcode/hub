{
  "name": "hub",
  "publisher": "lcode",
  "displayName": "lcode hub",
  "icon": "icon.png",
  "description": "",
  "version": "0.1.5",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onUri",
    "onFileSystem:webdav"
  ],
  "main": "./extension.js",
  "repository": {
